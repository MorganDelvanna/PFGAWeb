<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.8" />
    <httpRuntime targetFramework="4.8" />
  </system.web>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="pfga_php">
                    <match url="(.*\.php$)" />
                    <action type="Rewrite" url="http://localhost:8000/{R:0}" logRewrittenUrl="true" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>